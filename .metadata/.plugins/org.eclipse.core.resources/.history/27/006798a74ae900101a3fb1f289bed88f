package multiexample;

import java.util.Scanner;

public class String_example03 {

	public static void main(String[] args) {
		
		String[] names = {"홍길동", "김유신", "마동석", "자바킹", "서동요"};
		String minNames=0;
		int index = 0;
		for(int i=0; i<names.length; i++) {
			minNames = names[i];
			index = i;
			
		}

	}

}
