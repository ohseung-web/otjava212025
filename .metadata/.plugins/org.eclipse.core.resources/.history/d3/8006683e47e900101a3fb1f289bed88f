package multiexample;

import java.util.Scanner;

public class String_example02 {

	public static void main(String[] args) {
		// String 객체 
		Scanner scan = new Scanner(System.in);
		//2개의 문자열을 비교해 int형 값으로 리턴(반환)해주는 함수이다.
//		A.compareTo(B)
//		결과
//		     A와 B가 같으면				0
//		     A가 B보다 사전적으로 순서가 앞이면		음수
//		     A보다 B가 사전적으로 순서가 앞이면		양수
//		     (단, 다른 문자인 경우 해당 문자의 아스키코드의 차이를 출력한다.)

		String str1 = "가";
		String str2 = "나";
		String str3 = "가";

		int rs1 = str1.compareTo(str2);
		System.out.println(rs1);// 음수

		int rs2 = str2.compareTo(str1);
		System.out.println(rs2);// 양수

		int rs3 = str1.compareTo(str3);
		System.out.println(rs3);// 0
		
		String str4 = "abcd";
		String str5 = "ab";
		String str6 = "zyxvw";
		String str7 = "abcd";

		int rs4 = str4.compareTo(str5);
		System.out.println(rs1);// 2

		int rs5 = str2.compareTo(str4);
		System.out.println(rs2);// -2

		int rs6 = str1.compareTo(str6);
		System.out.println(rs3);// -25

		int rs7 = str1.compareTo(str7);
		System.out.println(rs4);// 0

		
	}

}
