package multiexample;

public class MultiArrEx01 {

	public static void main(String[] args) {
		// 다차원 배열 선언 방법
		// int[][] arr = new int[2][3]
        int[][] arr = {{1,2,3},{4,5,6}};
        
        for(int i=0; i<arr.length; i++) {
        	for(int j=0; j<arr[0].length; j++) {
        		System.out.print(arr[i][j]);
        	}
        	System.out.println();
        }
	}

}
