package multiexample;

import java.util.Scanner;

public class String_example03 {

	public static void main(String[] args) {
		
		String[] names = {"홍길동", "김유신", "마동석", "자바킹", "서동요"};
		String minNames= null;
		
		int index = 0;
		String temp = null;
		for(int i=0; i<names.length; i++) {
			
			minNames = names[i];
			index = i;
			
			for(int j=i; j<names.length; j++) {
				if(minNames.compareTo(names[j])>0) {
					minNames = names[j];
					index = j;
				}
			}
			temp = names[i];
			names[i]=names[index];
			names[index]=temp;
		}

		//출력
		for(int i=0; i<names.length; i++) {
			System.out.print(names[i]+" ");
		}
	}

}
